{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "systemName"            : "PI_RGB",
        "navType"               : "vertical",
        "theme"                 : "yeti",
        "linenums"              : true,
        "collapseSymbols"       : true,
        "inverseNav"            : true,
        "outputSourceFiles"     : true ,
        "outputSourcePath"      : true,
        "syntaxTheme"           : "dark",
        "search"                : true,


        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "staticFiles": {
                "paths": ["./documentation/images"]
            },
            "useLongnameInNav": true
        }
    }
}